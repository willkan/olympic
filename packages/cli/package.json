{
  "name": "olympic-cli",
  "version": "0.1.1",
  "author": "willkan @willkan",
  "bin": {
    "olympic-cli": "./bin/run"
  },
  "bugs": "https://github.com/willkan/olympic/olympic-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.4.14",
    "@oclif/config": "^1.6.13",
    "@oclif/plugin-help": "^1.2.5",
    "fs-extra": "^5.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.4",
    "@oclif/test": "^1.0.5",
    "@oclif/tslint": "^1.1.0",
    "@types/chai": "^4.1.3",
    "@types/fs-extra": "^5.0.2",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.5",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "ts-node": "^5.0.1",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/.oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/willkan/olympic/olympic-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "olympic-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
